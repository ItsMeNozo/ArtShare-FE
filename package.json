{
  "name": "art-share",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:test": "cross-env MODE=test vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "format": "prettier --write .",
    "test": "cross-env TEST_ENV=local playwright test --project=e2e-chrome",
    "test:ui": "cross-env TEST_ENV=local playwright test --ui --trace on",
    "test:production": "cross-env TEST_ENV=production playwright test --project=production-safe --trace on --headed",
    "test:preview": "cross-env TEST_ENV=preview playwright test --project=unsafe --trace on --headed",
    "test:ai": "cross-env TEST_ENV=local playwright test --project=ai --trace on --headed",
    "test:cleanup": "cross-env TEST_ENV=preview playwright test --project=cleanup --trace on",
    "test:production:ci": "cross-env TEST_ENV=production playwright test --project=production-safe --reporter=line,html",
    "test:preview:ci": "cross-env TEST_ENV=preview playwright test --project=unsafe --reporter=line,html",
    "test:cleanup:ci": "cross-env TEST_ENV=preview playwright test --project=cleanup",
    "test:all:ci": "npm run test:production:ci && npm run test:preview:ci",
    "test:smoke:ci": "cross-env TEST_ENV=production playwright test --project=production-safe --grep @smoke",
    "test:report": "playwright show-report"
  },
  "prepare": "husky",
  "lint-staged": {
    "*.{js,ts,jsx,tsx,json,css,md}": [
      "prettier --write --ignore-unknown"
    ]
  },
